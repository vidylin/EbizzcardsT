<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:ligo="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <com.gzligo.ebizzcardstranslator.common.ToolActionBar
        android:id="@+id/agreement_actionbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ligo:show_back_icon="true"
        ligo:txt_center_title="@string/user_service_agreement_title"/>

    <com.gzligo.ebizzcardstranslator.common.ProgressWebView
        android:id="@+id/agreement_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
